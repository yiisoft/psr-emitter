{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php:\/\/stderr",
        "stryker": {
            "report": "master"
        }
    },
    "minCoveredMsi": 100,
    "mutators": {
        "@default": true,
        "LessThanOrEqualTo": {
            "ignoreSourceCodeByRegex": [
                "if \\(\\$size !== null && \\$size <= \\$this->bufferSize\\) {"
            ]
        }
    }
}
